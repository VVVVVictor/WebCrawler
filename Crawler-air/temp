var timerID=null; 
var timerRunning=false; 
file://启动 
function startclock() 
{ 
stopclock(); 
time(); 
} 

file://停止 
function stopclock() 
{ 
if(timerRunning) 
clearTimeout(timerID); 
timerRunning=false; 
} 

file://实现模块 
function time() 
{ 
file://使用new操作符创建时间对象 
var now=new Date(); 
var yr=now.getYear(); 
var mName=now.getMonth()+1; 
var dayNr=((now.getDate()<10)?"0":"")+now.getDate(); 
var dName=now.getDay(); 
var ampm=(now.getHours()>=12)?"下午":"上午" 
var hours=now.getHours(); 
var minutes=((now.getMinutes()<10)?":0":":")+now.getMinutes(); 
var seconds=((now.getSeconds()<10)?":0":":")+now.getSeconds(); 

file://判断月份 
if(mName==1)Month="01"; 
if(mName==2)Month="02"; 
if(mName==3)Month="03"; 
if(mName==4)Month="04"; 
if(mName==5)Month="05"; 
if(mName==6)Month="06"; 
if(mName==7)Month="07"; 
if(mName==8)Month="08"; 
if(mName==9)Month="09"; 
if(mName==10)Month="10"; 
if(mName==11)Month="11"; 
if(mName==12)Month="12"; 
if(dName==1)dName="一"; 
if(dName==2)dName="二"; 
if(dName==3)dName="三"; 
if(dName==4)dName="四"; 
if(dName==5)dName="五"; 
if(dName==6)dName="六"; 
if(dName==0)dName="日"; 
if(yr<1000) yr=yr+1900;

var MonthDayYear=(""+yr+"年"+Month+"月"+dayNr+"日  星期"+dName); 
var TimeValue=(""+hours+minutes); 
document.getElementById("dateTime").innerHTML = MonthDayYear+" "+TimeValue;
//document.all.dateTimeValue.innerHTML=MonthDayYear+" "+TimeValue;
timerID=setTimeout("time()",1000); 
timerRunning=true; 
} 
