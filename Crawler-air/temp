	function report1_toPage( pageNo ) {
	if( pageNo < 1 || pageNo > report1_getTotalPage() ) return;
		document.report1_turnPageForm.report1_currPage.value = pageNo;
		document.report1_turnPageForm.submit();
	}
</script>
