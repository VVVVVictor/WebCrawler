		<script language="javascript">
		var lang = '';
	function jump(maxp){
		var err1 ="页码必须是整数！";
		var err2 ="页码超出范围！";
		var url = "air_dairy_aqi.jsp?city=&startdate=2010-01-01&enddate=2010-01-01";
		var page = document.getElementById("pageNum").value;
		if(checkIsInteger(page))
			alert(err1);
		else if(parseInt(page)>parseInt(maxp) || parseInt(page)<=0)
			alert(err2);
		else{
			url += "&page="+page;
			window.location.href = url;
		}
	}
	

	function jumpTo(p){
		var url = "air_dairy_aqi.jsp?city=&startdate=2010-01-01&enddate=2010-01-01";
		url += "&page="+p;
		window.location.href = url;
	}
	function forwardToMain(){
		//document.location.href = "http://datacenter.mep.gov.cn:80/report/";
		document.location.href = "http://datacenter.mep.gov.cn:80/";
		
	}
    function checkForm1(){
    	var date = document.getElementById('startdate').value;
    	var stopDate = document.getElementById('enddate').value;
    	if(date!=""&&stopDate!=""){
    		date = date.replace(/-/g,'/');
    		stopDate = stopDate.replace(/-/g,'/'); 
    	 	var d1=new Date(date);   
  			var d2=new Date(stopDate);
			if(Date.parse(d1) - Date.parse(d2)>0){	
			    return false;
			}
  		}
  		if(date!=""&&stopDate==""){
  			alert('您没有输入结束时间，请重新输入！');
  			return false;
  		}
  		if(date==""&&stopDate!=""){
  			alert('您没有输入开始时间，请重新输入！');
  			return false;
  		}
    	return true;
    }
    
</script>
